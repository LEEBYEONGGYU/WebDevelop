<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.busyvacation.happyclass.db.mapper.UserMapper">
<select id="getUsers" resultType="com.busyvacation.happyclass.db.dto.User">
select * from userlist
</select>
<select id="getUser" resultType="com.busyvacation.happyclass.db.dto.User">
select * from userlist where userName = #{id}
</select>
</mapper>