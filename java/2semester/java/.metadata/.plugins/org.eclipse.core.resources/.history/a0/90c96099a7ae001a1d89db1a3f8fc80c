package android_test_code;

public class Main {

	static double distance;
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		distance = 35.5825874;
		int a = (int)distance;
		
		String test = String.valueOf(a);
		
		
		int leget = test.length();
		
		
		
		
		if(leget==2) {
			String get = test.substring(test.length()-2);
			String get2 = get.substring(0,1);
			System.out.println(get2);
			if(get2.equals(7)) {
				System.out.println("70m주의구간");
			}
		}else if(leget==1) {System.out.println("지금1임");}
		
		
	
		
	}

}
